<?xml version="1.0" encoding="UTF-8" ?>
<module>
  <ModulePrefs title="Learning Resources" />

  <Content type="html" view="home">

    <![CDATA[     
       
     <style type="text/css">
         .imgBox
         {
             width: 130px;
             height: 55px;
         }
         
         .imgBox:hover
         {
             border: solid 1px #CCC;
             -moz-box-shadow: 1px 1px 5px #999;
             -webkit-box-shadow: 1px 1px 5px #999;
             box-shadow: 1px 1px 5px #999;
         }
         
         
         .fontfamily
         {
             font-family: arial, helvetica, sans-serif;
             font-size: 13pt;
         }
         .fontfamily2
         {
             font-family: arial, helvetica, sans-serif;
             font-size: 9pt;
         }
         
         .outerTable
         {
             width: 40%;
             height: 250px;
         }
         
         .hrln
         {
             height: 1.5px;
             border: none;
             color: #333;
             background-color: #333;
         }
     </style>

  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js">
    </script>
    <script language="javascript" type="text/javascript">
        linkHper();
        function linkHper() {

            var link;
            var flink1;
            var flink2;

            $.get("res/link/LinkURL.txt",
               function (txt, status, jqXHR) {

                   var lines = txt.split("\n");
                   for (var i = 0, len = lines.length; i < len; i++) {

                       link = lines[i].split('=');
                       flink1 = link[0];
                       flink2 = link[1];
                       document.getElementById(flink1).href = flink2;
                   }
               },
        "text"
     );

   }
    
    </script>
    <table class="outerTable">
        <tr>
            <td colspan="2">
                <table width="100%">
                    <tr>
                        <td width="32%">
                            <hr class="hrln" />
                        </td>
                        <td width="36%" align="center">
                            <span style="font-size: 13pt">Technical Learning Resources</span>
                        </td>
                        <td width="34%">
                            <hr class="hrln" />
                        </td>
                    </tr>
                </table>
                
            </td>
        </tr>
    </table>
    
    ]]>

    
  </Content>
  <Content type="html" view="canvas">
    <![CDATA[    
      
     ]]>
  </Content>
</module>
